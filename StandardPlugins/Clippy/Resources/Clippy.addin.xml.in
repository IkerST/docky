<Addin 
	id="Clippy"
	namespace="Docky"
	version="1.0"
	isroot="false"
	defaultEnabled="false"
	name="Clippy"
	description="Keep recent clipboard entries."
	author="Robert Dyer"
	icon="edit-cut"
>
 
 	<Dependencies>
		<Addin id="Items" version="1.0" />
	</Dependencies>

	<Runtime>
		<Import assembly="Clippy.dll" />
	</Runtime>

	<Extension path="/Docky/ItemProvider">
		<ItemProvider type="Clippy.ClippyItemProvider" />
	</Extension>
</Addin>
